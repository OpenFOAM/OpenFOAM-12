FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      scale(age);
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 1 0 0 0 0];

internalField   uniform 0.0125;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0.0125;
    }
    walls
    {
        type            calculated;
        value           uniform 0.0125;
    }
    frontAndBack
    {
        type            empty;
    }
}


