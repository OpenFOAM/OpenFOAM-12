FoamFile
{
    format      ascii;
    class       pointScalarField;
    location    "0";
    object      streamFunction;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -1 0 0 0 0];

internalField   nonuniform List<scalar> 8(0 0 9.268983952 9.268983952 0 0 9.268983952 9.268983952);

boundaryField
{
    inlet
    {
        type            calculated;
    }
    outlet
    {
        type            calculated;
    }
    walls
    {
        type            calculated;
    }
    frontAndBack
    {
        type            empty;
    }
}


