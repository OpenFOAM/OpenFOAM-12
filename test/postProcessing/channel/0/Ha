FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Ha;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1894.95;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 1894.95;
    }
    outlet
    {
        type            calculated;
        value           uniform 1894.95;
    }
    walls
    {
        type            calculated;
        value           uniform 1862.95;
    }
    frontAndBack
    {
        type            empty;
    }
}


