FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      total(kinematic(p));
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 103609.6226;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 103609.6226;
    }
    outlet
    {
        type            calculated;
        value           uniform 103609.6226;
    }
    walls
    {
        type            calculated;
        value           uniform 103571.2226;
    }
    frontAndBack
    {
        type            empty;
    }
}


