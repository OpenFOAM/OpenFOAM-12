FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "1";
    object      vorticity;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform (0 -0 3.370488911e-09);

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform (0 -0 6.740977822e-09);
    }
    outlet
    {
        type            calculated;
        value           uniform (0 -0 0);
    }
    walls
    {
        type            calculated;
        value           nonuniform List<vector> 2((0 -0 -17.25884779) (0 -0 17.2588478));
    }
    frontAndBack
    {
        type            empty;
    }
}


