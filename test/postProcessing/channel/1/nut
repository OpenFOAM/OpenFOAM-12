FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "1";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.06372387011;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0.2894898102;
    }
    outlet
    {
        type            calculated;
        value           uniform 0.06372387011;
    }
    walls
    {
        type            nutkWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           nonuniform List<scalar> 2(0.005204631419 0.00520468165);
    }
    frontAndBack
    {
        type            empty;
    }
}


