FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "1";
    object      uFrac;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (0.0006233761446 7.083000861e-11 0);

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform (6.753162989e-06 0 0);
    }
    outlet
    {
        type            calculated;
        value           uniform (0.0006233761446 7.083000861e-11 0);
    }
    walls
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    frontAndBack
    {
        type            empty;
    }
}


