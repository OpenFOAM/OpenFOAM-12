FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "1";
    object      u;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.005379377 6.11222169e-10 0);

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform (5.8286e-05 0 0);
    }
    outlet
    {
        type            calculated;
        value           uniform (0.005379377 6.11222169e-10 0);
    }
    walls
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    frontAndBack
    {
        type            empty;
    }
}


