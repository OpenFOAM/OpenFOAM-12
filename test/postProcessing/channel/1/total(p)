FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "1";
    object      total(p);
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100043.3322;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 100043.3472;
    }
    outlet
    {
        type            calculated;
        value           uniform 100043.1396;
    }
    walls
    {
        type            calculated;
        value           nonuniform List<scalar> 2(100005.876 99994.50987);
    }
    frontAndBack
    {
        type            empty;
    }
}


