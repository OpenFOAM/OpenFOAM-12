FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "1";
    object      ddt(p);
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -3 0 0 0 0];

internalField   uniform -8.654023986e-06;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform -8.654023986e-06;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    walls
    {
        type            calculated;
        value           nonuniform List<scalar> 2(-8.936331142e-06 -8.373172022e-06);
    }
    frontAndBack
    {
        type            empty;
    }
}


