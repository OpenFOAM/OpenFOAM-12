FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "1";
    object      epsilonMean;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.1128997179;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0.02428740899;
    }
    outlet
    {
        type            calculated;
        value           uniform 0.1128997179;
    }
    walls
    {
        type            calculated;
        value           uniform 0.120872207;
    }
    frontAndBack
    {
        type            empty;
    }
}


