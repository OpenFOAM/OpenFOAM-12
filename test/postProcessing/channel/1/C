FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "1";
    object      C;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform (-0.5 0 0);
    }
    outlet
    {
        type            calculated;
        value           uniform (0.5 0 0);
    }
    walls
    {
        type            calculated;
        value           nonuniform List<vector> 2((0 -0.5 0) (0 0.5 0));
    }
    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
