FoamFile
{
    format      ascii;
    class       dictionary;
    location    "1/uniform";
    object      fieldAverage(U,k,epsilon)Properties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

U
{
    totalIter       100;
    totalTime       1;
}

k
{
    totalIter       100;
    totalTime       1;
}

epsilon
{
    totalIter       100;
    totalTime       1;
}


