FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "1";
    object      Ta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 297;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 297;
    }
    outlet
    {
        type            calculated;
        value           uniform 297;
    }
    walls
    {
        type            calculated;
        value           uniform 297;
    }
    frontAndBack
    {
        type            empty;
    }
}


