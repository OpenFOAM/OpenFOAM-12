FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "1";
    object      alpha.dummy;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.2;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0.2;
    }
    outlet
    {
        type            calculated;
        value           uniform 0.2;
    }
    walls
    {
        type            calculated;
        value           uniform 0.2;
    }
    frontAndBack
    {
        type            empty;
    }
}


