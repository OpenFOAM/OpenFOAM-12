FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "1";
    object      mag(smallU);
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform 1e-16;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 1e-16;
    }
    outlet
    {
        type            calculated;
        value           uniform 1e-16;
    }
    walls
    {
        type            calculated;
        value           uniform 1e-16;
    }
    frontAndBack
    {
        type            empty;
    }
}


