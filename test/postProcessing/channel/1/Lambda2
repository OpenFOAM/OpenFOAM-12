FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "1";
    object      Lambda2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -2 0 0 0 0];

internalField   uniform -0;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform -0;
    }
    outlet
    {
        type            calculated;
        value           uniform -0;
    }
    walls
    {
        type            calculated;
        value           nonuniform List<scalar> 2(-5.819089263e-08 -0);
    }
    frontAndBack
    {
        type            empty;
    }
}


