FoamFile
{
    format      ascii;
    class       surfaceScalarField;
    location    "1";
    object      phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -1 0 0 0 0];

internalField   nonuniform List<scalar> 0();

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform -1;
    }
    outlet
    {
        type            calculated;
        value           uniform 1.000001188;
    }
    walls
    {
        type            calculated;
        value           nonuniform List<scalar> 2(-5.914217219e-07 -5.912872942e-07);
    }
    frontAndBack
    {
        type            empty;
    }
}


