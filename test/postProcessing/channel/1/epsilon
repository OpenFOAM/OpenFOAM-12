FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "1";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.1115927051;

boundaryField
{
    inlet
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    1;
        phi             phi;
        k               k;
        value           uniform 0.02426050544;
    }
    outlet
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    1;
        phi             phi;
        k               k;
        value           uniform 0.1115927051;
    }
    walls
    {
        type            epsilonWallFunction;
        value           uniform 0.1194538473;
    }
    frontAndBack
    {
        type            empty;
    }
}


