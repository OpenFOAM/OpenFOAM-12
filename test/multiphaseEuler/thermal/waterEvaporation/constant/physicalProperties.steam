thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState perfectGas;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        molWeight       18.0153;
    }
    thermodynamics
    {
        hf              -1.34229e+07;
        Cp              2078.4;
        Cv              1616.9;
        Tref            372.76;
        hsRef           128652;
        esRef           -43381.7;
    }
    transport
    {
        mu              1.2256e-05;
        Pr              2.289;
    }
}


