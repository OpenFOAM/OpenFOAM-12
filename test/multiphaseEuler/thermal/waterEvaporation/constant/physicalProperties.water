{
    specie
    {
        molWeight       18.0153;
    }
    equationOfState
    {
        rho             1027;
    }
    thermodynamics
    {
        hf              -1.58643e+07;
        Cv              4195;
        Cp              4195;
        Tref            372.76;
        hsRef           312508;
        esRef           312410;
    }
    transport
    {
        mu              0.00028291;
        Pr              2.289;
    }
}


