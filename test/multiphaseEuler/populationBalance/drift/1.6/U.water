FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "1.6";
    object      U.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    boundary
    {
        type            empty;
    }
}

sources
{
    waterSink
    {
        type            internal;
    }
}


