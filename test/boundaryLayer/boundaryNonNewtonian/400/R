FoamFile
{
    format      ascii;
    class       volSymmTensorField;
    location    "400";
    object      R;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform (0 0 0 0 0 0);

boundaryField
{
    lowerWall
    {
        type            calculated;
        value           uniform (0 0 0 0 0 0);
    }
    upperWall
    {
        type            calculated;
        value           uniform (0 0 0 0 0 0);
    }
    front
    {
        type            cyclic;
    }
    back
    {
        type            cyclic;
    }
    defaultFaces
    {
        type            empty;
    }
}


