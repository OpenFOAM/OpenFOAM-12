FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0.005";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    zeroSizedPatch
    {
        type            noSlip;
    }
    movingWall
    {
        type            fixedValue;
        value           nonuniform List<vector> 0();
    }
    fixedWalls
    {
        type            noSlip;
    }
    frontAndBack
    {
        type            empty;
    }
    myWall
    {
        type            calculated;
        value           nonuniform List<vector> 0();
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


// ************************************************************************* //
