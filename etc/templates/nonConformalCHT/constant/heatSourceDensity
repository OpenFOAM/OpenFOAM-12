FoamFile
{
    format      ascii;
    class       dictionary;
    object      heatSource;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type        heatSource;

select      all;

// Power per unit volume [W/m^3]
q          <10>;

