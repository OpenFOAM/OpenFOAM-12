FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 1000;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1000;
        value           uniform 1000;
    }
    walls
    {
        type            zeroGradient;
    }
    procBoundary4to0
    {
        type            processor;
        value           uniform 1000;
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform 1000;
    }
}


