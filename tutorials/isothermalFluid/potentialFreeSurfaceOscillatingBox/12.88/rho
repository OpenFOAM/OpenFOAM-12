FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "12.88";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1000;

boundaryField
{
    freeSurface
    {
        type            calculated;
        value           uniform 1000;
    }
    walls
    {
        type            calculated;
        value           uniform 1000;
    }
    floatingObject
    {
        type            calculated;
        value           uniform 1000;
    }
    frontAndBack
    {
        type            empty;
    }
}


