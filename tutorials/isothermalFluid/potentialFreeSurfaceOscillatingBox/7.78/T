FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "7.78";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    freeSurface
    {
        type            calculated;
        value           uniform 300;
    }
    walls
    {
        type            calculated;
        value           uniform 300;
    }
    floatingObject
    {
        type            calculated;
        value           uniform 300;
    }
    frontAndBack
    {
        type            empty;
    }
}


