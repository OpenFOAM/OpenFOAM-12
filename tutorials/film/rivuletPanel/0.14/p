FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0.14";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    sides
    {
        type            zeroGradient;
    }
    wall
    {
        type            zeroGradient;
    }
    empty
    {
        type            empty;
    }
}


